{
    "name": "Ryanator",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Are you a Veep fan? Do you actually believe what Jonah Ryan says? Well, now you can manifest that on your compooter!",
    "homepage_url": "",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["src/inject/inject.js"]
      }
    ]
}